{
  "name": "talaria",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release": "changeset publish",
    "build": "nx run-many --target=build --projects=@talaria/cli,@talaria/render,@talaria/components --parallel",
    "lint:all": "pnpm run lint:packages && pnpm run lint:test && pnpm run lint:examples && pnpm run lint:docs",
    "lint:packages": "nx run-many --target=lint --all --parallel",
    "lint:test": "cd test && pnpm run lint",
    "lint:examples": "cd examples && pnpm run lint",
    "lint:docs": "eslint docs/.vitepress  --ext .ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test": "pnpm -r run test --run",
    "test:watch": "pnpm -r run test",
    "commitlint": "commitlint",
    "cz": "cz",
    "lint-staged": "lint-staged",
    "changeset": "changeset",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@nrwl/nx-cloud": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.31.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "nx": "15.4.4",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4",
    "vitepress": "1.0.0-alpha.35",
    "vue": "^3.2.45"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": [
      "eslint"
    ]
  }
}
